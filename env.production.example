# Production Environment Variables for Vercel
# Copy these to your Vercel project environment variables

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_production_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# App Configuration
NEXT_PUBLIC_APP_NAME="Modern Template"
NEXT_PUBLIC_APP_DESCRIPTION="A modern, beautiful website template built with Next.js, Supabase, and Tailwind CSS"
NEXT_PUBLIC_APP_URL=https://your-vercel-domain.vercel.app

# Authentication Redirect URLs
NEXT_PUBLIC_AUTH_REDIRECT_URL=https://your-vercel-domain.vercel.app/auth/callback

# Analytics (optional)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=

# Email (optional)
RESEND_API_KEY=
FROM_EMAIL=

# Database URLs (for production)
DATABASE_URL=
DIRECT_URL=
